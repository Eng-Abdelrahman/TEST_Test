
@{
    ViewBag.Title = "Register Success!";
    Layout = null;
}

<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link href="https://fonts.googleapis.com/css?family=Poppins:200,300,400,600,700,800" rel="stylesheet" />
    <link href="https://use.fontawesome.com/releases/v5.0.6/css/all.css" rel="stylesheet">
    <!-- Nucleo Icons -->
    <link href="~/Theme/assets/css/nucleo-icons.css" rel="stylesheet" />
    <!-- CSS Files -->
    <link href="~/Theme/assets/css/black-dashboard.css?v=1.0.0" rel="stylesheet" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-rtl/3.4.0/css/bootstrap-rtl.css" rel="stylesheet" />

    <style>
        div#container {
            margin-right: 6.333333%;
            margin-top: 8%;
        }
    </style>

</head>
<body class=" rtl menu-on-right white-content ">
    <div class="wrapper">
        <div class="main-panel">
            <div class="content">
                <div class="row">
                    <div id="container" class="col-9 col-lg-offset-1">
                        <div class="card ">
                            <div class="card-header ">
                                <div class="row">
                                    <div class="col-sm-6 text-right">
                                        <h5 class="card-category"></h5>
                                        <h5 class="title">تم تسجيل الدخول بنجاح</h5>
                                    </div>
                                </div>
                            </div>

                            <div class="card-body">
                                <div class="alert alert-success alert-dismissible" role="alert">
                                    <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                                    <p>تم إنشاء الحساب بنجاح</p>
                                </div>

                            </div>

                            <div class="card-footer">

                                <div class="row">

                                    <div class="col-md-offset-1">
                                        <div class="form-group">
                                            <input type="submit" value="Log in" class="btn btn btn-fill btn-primary" form="loginForm" style="margin-right:15%;" />
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>


    <script src="~/Theme/assets/js/core/jquery.min.js"></script>
    <script src="~/Theme/assets/js/core/popper.min.js"></script>
    <script src="~/Theme/assets/js/core/bootstrap.min.js"></script>
    <script src="~/Theme/assets/js/plugins/perfect-scrollbar.jquery.min.js"></script>
    @Scripts.Render("~/bundles/jqueryval")
    <!--  Notifications Plugin    -->
    <script src="~/Theme/assets/js/plugins/bootstrap-notify.js"></script>
    <!-- Control Center for Black Dashboard: parallax effects, scripts for the example pages etc -->
    <script src="~/Theme/assets/js/black-dashboard.min.js?v=1.0.0"></script>

</body>


</html>




